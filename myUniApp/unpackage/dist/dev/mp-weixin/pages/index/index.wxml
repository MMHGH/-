<view class="uniApp"><view class="text-area"><text class="title">{{"当前所在页面"+page}}</text></view><view class="content"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index"><text>{{item}}</text></block></view><view class="link-list"><button type="primary" size="mini" data-event-opts="{{[['tap',[['goList',['list']]]]]}}" bindtap="__e">跳转到列表页</button><button class="mini-btn" type="warn" size="mini" data-event-opts="{{[['tap',[['goList',['tips']]]]]}}" bindtap="__e">跳转到消息页</button></view><view data-event-opts="{{[['tap',[['goTop',['$event']]]]]}}" class="go-top" bindtap="__e">返回顶部</view><view><toast vue-id="1" flag="{{flag}}" bind:__l="__l"></toast></view><view><text>{{"转换后的时间:"+date}}</text></view><view><uni-grid vue-id="2" column="{{3}}" bind:__l="__l" vue-slots="{{['default']}}"><uni-grid-item vue-id="{{('3')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}"><text class="text">文本</text></uni-grid-item><uni-grid-item vue-id="{{('4')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}"><text class="text">文本</text></uni-grid-item><uni-grid-item vue-id="{{('5')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}"><text class="text">文本</text></uni-grid-item></uni-grid></view></view>